<root>
	<styles>
		<include src="file://{resources}/styles/custom_game/material_design.css" />	
		<include src="file://{resources}/styles/custom_game/game_setup.css" />
	</styles>
	<scripts>
		<include src="file://{resources}/scripts/custom_game/game_setup/game_setup_vote_utils.js" />
	</scripts>
	<Panel hittest="false" class="VoteContainer rounded z-index1">
		<Panel class="VoteContainer" desc="#game_setup_use_miniactors_vote" id="Miniactors" >
			<Panel class="DarkTheme Primary500" id="VoteHeader" >
				<Label class="VoteQuestionLabel" text="#vote_use_miniactors_q" />
			</Panel>
			<Panel class="DarkTheme Cards" id="VoteVariants" param="use_miniactors">
				<Button class="DarkTheme FlatButton" value="true" default="true">
					<Label class="DarkTheme PrimaryText FlatLabel" text="#vote_use_miniactors_use" />
				</Button>
				<Button class="DarkTheme FlatButton" value="false">
					<Label class="DarkTheme PrimaryText FlatLabel" text="#vote_use_miniactors_not_use" />
				</Button>
			</Panel>
		</Panel>
	</Panel>
</root>

